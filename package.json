{
  "name": "eye-tracking-aac",
  "version": "3.0.0",
  "description": "MediaPipe視線追跡式意思伝達装置 - Eye-Tracking AAC Device for Japanese",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:stable": "vite --port 3001 --host",
    "dev:alt": "vite --port 3002 --host",
    "build": "vite build",
    "build:github": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "start": "vite dev --host",
    "serve": "vite preview --host",
    "deploy": "npm run build:github && git add dist && git commit -m 'Deploy to GitHub Pages' && git subtree push --prefix dist origin gh-pages"
  },
  "keywords": [
    "aac",
    "assistive-technology",
    "eye-tracking",
    "accessibility",
    "japanese",
    "communication-aid",
    "mediapipe",
    "face-tracking",
    "speech-synthesis",
    "vue3"
  ],
  "author": "AAC Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/eye-tracking-aac.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/eye-tracking-aac/issues"
  },
  "homepage": "https://YOUR_USERNAME.github.io/eye-tracking-aac",
  "dependencies": {
    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/face_detection": "^0.4.1646425229",
    "@vueuse/core": "^10.7.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions"
  ]
}
